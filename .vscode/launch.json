  "version": "0.2.0",
  "configurations": [
    {
      "name": "(Windows) lib_tests",
      "type": "cppvsdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build_codetest/Debug/lib_tests",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${fileDirname}",
      "environment": [],
      "console": "externalTerminal"
    },
    {
      "name": "(gdb) MySTM32",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build_mystm32/zephyr/zephyr.elf",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "miDebuggerPath": "D:/workspace/toolchain/gcc/bin/arm-none-eabi-gdb.exe",
      "miDebuggerServerAddress": "localhost:3333",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}